{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug C++",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/src/out/mainCppDebug",
        "args": [
          "/home/mateus/WSL/IC/SSP/input/MyInstancesSameToolSets/n=75,p=0.24,r=0.5,t=650,v0.csv",
          "/home/mateus/WSL/IC/SSP/input/ToolSetInt.csv",
